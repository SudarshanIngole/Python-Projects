   "source": [
    "# calculating BMI\n",
    "\n",
    "height=input('Enter Height in meter  ' )\n",
    "weight=input('Enter Weight in kg  ' )\n",
    "\n",
    "new_height=float(height)\n",
    "new_weight=int(weight)\n",
    "\n",
    "BMI=new_weight/new_height**2\n",
    "\n",
    "print('Body Mass Index is ',round(BMI,2))\n"
   ]
